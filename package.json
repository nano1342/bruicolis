{
  "name": "music-playlist-api",
  "version": "1.0.0",
  "description": "APP5 API project for handling music and their tags",
  "main": "index.ts",
  "type": "commonjs",
  "scripts": {
    "build": "npx tsc",
    "dev": "nodemon",
    "start": "node dist/index.js",
    "import:musicbrainz": "npx tsc && node dist/scripts/import_musicbrainz_csv.js",
    "prisma:dev:reset": "npx prisma db push --force-reset && npx prisma db push",
    "import:musicbrainz:with_reset": "npm run prisma:dev:reset && npm run import:musicbrainz"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.3.1",
    "better-sqlite3": "^11.8.1",
    "csv-parse": "^5.6.0",
    "express": "^4.21.2",
    "joi": "^17.13.3"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.10",
    "@types/pg": "^8.11.11",
    "nodemon": "^3.1.9",
    "pg": "^8.13.1",
    "prisma": "^6.3.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  }
}
